        {
          "dataset": "iox",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-v3-sql"
          },
          "editorMode": "code",
          "format": "time_series",
          "query": "SELECT date_bin(INTERVAL '$__interval', time) AS t,\\n  mean(\"user\") AS cpu_user,\\n  mean(\"nice\") AS cpu_nice,\\n  mean(\"system\") AS cpu_system,\\n  mean(\"irq\") AS cpu_irq,\\n  mean(\"softirq\") AS cpu_softirq,\\n  mean(\"iowait\") AS cpu_iowait,\\n  mean(\"steal\") AS cpu_steal\\nFROM \\\"public\\\".\\\"metrics\\\".\\\"cpu\\\"\\nWHERE cpu = 'cpu' AND host = '${host}' AND $__timeFilter(time)\\nGROUP BY t\\nORDER BY t",
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  hostname,\n  time,\n  ROUND(\n    100.0 * (total_delta - idle_delta) / total_delta,\n    2\n  ) AS cpu_utilization_percent\nFROM (\n  SELECT\n    hostname,\n    time,\n    -- 计算总 CPU 时间差值\n    (\"user\" - LAG(\"user\") OVER w) +\n    (nice - LAG(nice) OVER w) +\n    (system - LAG(system) OVER w) +\n    (iowait - LAG(iowait) OVER w) +\n    (irq - LAG(irq) OVER w) +\n    (softirq - LAG(softirq) OVER w) +\n    (steal - LAG(steal) OVER w) +\n    (idle - LAG(idle) OVER w) AS total_delta,\n    -- 计算空闲时间差值\n    (idle - LAG(idle) OVER w) AS idle_delta\n  FROM cpu_total\n  WHERE time >= $__timeFrom AND time < $__timeTo AND hostname = ANY(string_to_array('${host}', ','))\n  WINDOW w AS (\n    PARTITION BY hostname\n    ORDER BY time\n  )\n) AS deltas\nORDER BY time,hostname,cpu_utilization_percent;",
          "refId": "A",
          "sql": {

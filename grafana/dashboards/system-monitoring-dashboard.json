          "editorMode": "code",
          "format": "time_series",
          "query": "SELECT date_bin(INTERVAL '$__interval', time) AS t,\\n  cpu,\\n  (mean(\"user\") + mean(\"nice\") + mean(\"system\") + mean(\"irq\") + mean(\"softirq\") + mean(\"iowait\")) AS usage\\nFROM \\\"public\\\".\\\"metrics\\\".\\\"cpu\\\"\\nWHERE cpu != 'cpu' AND host = '${host}' AND $__timeFilter(time)\\nGROUP BY t, cpu\\nORDER BY t",
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  hostname,\n  cpu,\n  time,\n  ROUND(\n    100.0 * (total_delta - idle_delta) / total_delta,\n    2\n  ) AS cpu_utilization_percent\nFROM (\n  SELECT\n    hostname,\n    cpu,\n    time,\n    -- 计算总 CPU 时间差值\n    (\"user\" - LAG(\"user\") OVER w) +\n    (nice - LAG(nice) OVER w) +\n    (system - LAG(system) OVER w) +\n    (iowait - LAG(iowait) OVER w) +\n    (irq - LAG(irq) OVER w) +\n    (softirq - LAG(softirq) OVER w) +\n    (steal - LAG(steal) OVER w) +\n    (idle - LAG(idle) OVER w) AS total_delta,\n    -- 计算空闲时间差值\n    (idle - LAG(idle) OVER w) AS idle_delta\n  FROM cpu_core\n  WHERE time >= $__timeFrom AND time < $__timeTo AND hostname = ANY(string_to_array('${host}', ','))\n  WINDOW w AS (\n    PARTITION BY cpu,hostname\n    ORDER BY time\n  )\n) AS deltas\nORDER BY time,cpu,cpu_utilization_percent;",

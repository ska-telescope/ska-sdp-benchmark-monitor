          "editorMode": "code",
          "format": "time_series",
          "query": "SELECT date_bin(INTERVAL '$__interval', time) AS t,\\n  device,\\n  mean(\"sect-rd\") AS read,\\n  mean(\"sect-wr\") AS write\\nFROM \\\"public\\\".\\\"metrics\\\".\\\"disk\\\"\\nWHERE host = '${host}' AND $__timeFilter(time)\\nGROUP BY t, device\\nORDER BY t",
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  hostname,\n  time,\n  device,\n  CASE\n    WHEN date_part('epoch', time - lag(time) OVER w) = 0 THEN NULL\n    ELSE GREATEST(0, (sectors_read - lag(sectors_read) OVER w) / date_part('epoch', time - lag(time) OVER w))\n  END AS read_sectors_ps,\n  CASE\n    WHEN date_part('epoch', time - lag(time) OVER w) = 0 THEN NULL\n    ELSE GREATEST(0, (sectors_written - lag(sectors_written) OVER w) / date_part('epoch', time - lag(time) OVER w))\n  END AS write_sectors_ps\nFROM disk_stats\nWHERE time >= $__timeFrom AND time < $__timeTo AND hostname = ANY(string_to_array('${host}', ','))\nWINDOW w AS (PARTITION BY hostname, device ORDER BY time)\nORDER BY time, device;",

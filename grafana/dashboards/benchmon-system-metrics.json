          },
          "query": "from(bucket: \"metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"cpu_user\" or r[\"_measurement\"] == \"cpu_system\" or r[\"_measurement\"] == \"cpu_idle\")\n  |> filter(fn: (r) => r[\"cpu\"] == \"cpu\")\n  |> derivative(unit: 1s, nonNegative: true)\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> pivot(rowKey: [\"_time\", \"instance\"], columnKey: [\"_measurement\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({ \n      _time: r._time,\n      instance: r.instance,\n      cpu_usage_percent: (r.cpu_user + r.cpu_system) / (r.cpu_user + r.cpu_system + r.cpu_idle) * 100.0\n  }))\n  |> keep(columns: [\"_time\", \"instance\", \"cpu_usage_percent\"])",
          "refId": "A"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },
